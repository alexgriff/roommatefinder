<h1>Sign Up</h1>

<%= form_for @user do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation %><br>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>
  <%= f.label :age %>
  <%= f.select :age, (18..99) %>
  <%= f.label :gender %>
  <%= f.select :gender, ['M', 'F']%><br>
 
  <%= f.submit %>
<% end %>
